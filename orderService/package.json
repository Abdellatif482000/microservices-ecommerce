{
  "name": "order",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "swc src/* -d dist",
    "start": "NODE_ENV=prod node dist/src/index.js",
    "type-check": "tsc --noEmit",
    "order-swc-inMemory": "NODE_ENV=dev NODE_OPTIONS='--no-warnings' node --watch --experimental-loader @swc-node/register src/index.ts",
    "dev": "concurrently 'npm:type-check' 'npm:order-swc-inMemory'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@swc-node/core": "^1.13.3",
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.9",
    "@types/cookie-parser": "^1.4.8",
    "@types/cookies": "^0.9.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.7",
    "jsonwebtoken": "^9.0.2"
  },
  "dependencies": {
    "concurrently": "^9.1.2",
    "cookie-parser": "^1.4.7",
    "cookies": "^0.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "mongoose": "^8.9.5"
  }
}
